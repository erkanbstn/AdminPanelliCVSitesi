@model CV.Models.Entity.Deneyim
@{
    ViewBag.Title = "DeneyimGetir";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<br />

<h2 style="color:green;">Deneyim Güncelle</h2>
@using (Html.BeginForm("DeneyimGuncelle", "Yonetim", FormMethod.Post))
{

    <br />

    <div class="form-group">
        @Html.LabelFor(k => k.ID)
        <br />
        <br />
        @Html.TextBoxFor(k => k.ID, new { @class = "form-control" })
        <br />
        @Html.LabelFor(k => k.Baslik)
        <br />
        <br />
        @Html.TextBoxFor(k => k.Baslik, new { @class = "form-control" })
        <br />
        @Html.LabelFor(k => k.AltBaslik)
        <br />
        <br />
        @Html.TextBoxFor(k => k.AltBaslik, new { @class = "form-control" })
        <br />
        @Html.LabelFor(k => k.Aciklama)
        <br />
        <br />
        @Html.TextBoxFor(k => k.Aciklama, new { @class = "form-control" })
        <br />
        @Html.LabelFor(k => k.Tarih)
        <br />
        <br />
        @Html.TextBoxFor(k => k.Tarih, new { @class = "form-control" })
        <br />
        <br />
    </div>
    <div>
        <button class="btn btn-primary">Deneyim Güncelle</button>
    </div>
}



